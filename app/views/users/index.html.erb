<h1><%=pluralize(@users.size, "Users")%></h1>
    <ul class="users">
        <%= @users.each do |user|%>
            <li>
                <%= link_to user.name, user %>
                User signed up
                <%= time_ago_in_words(user.created_at) %> ago.
            </li>
        <%end%>
    </ul>
